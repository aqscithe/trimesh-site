    <footer class="trimesh-footer">
        <div class="container">
            <p>&copy; 2025 Trimesh Drone Solutions LLC. All rights reserved.</p>
        </div>
    </footer>

    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Hamburger/Drawer logic
            const hamburgerBtn = document.querySelector('.hamburger-btn');
            const drawer = document.querySelector('.services-drawer');
            const drawerClose = document.querySelector('.drawer-close');
            const drawerBackdrop = document.createElement('div');
            drawerBackdrop.className = 'drawer-backdrop';
            // Open drawer
            if (hamburgerBtn && drawer) {
              hamburgerBtn.addEventListener('click', function() {
                drawer.classList.add('open');
                document.body.appendChild(drawerBackdrop);
              });
            }
            // Close drawer
            if (drawerClose && drawer) {
              drawerClose.addEventListener('click', function() {
                drawer.classList.remove('open');
                if (drawerBackdrop.parentNode) drawerBackdrop.parentNode.removeChild(drawerBackdrop);
              });
            }
            drawerBackdrop.addEventListener('click', function() {
              if (drawer) drawer.classList.remove('open');
              if (drawerBackdrop.parentNode) drawerBackdrop.parentNode.removeChild(drawerBackdrop);
            });
            // DBA dropdown logic
            const dbaBtn = document.querySelector('.dba-btn');
            const dbaDropdown = document.querySelector('.dba-dropdown');
            const dbaArrow = document.querySelector('.dba-arrow svg');
            let dbaOpen = false;
            if (dbaBtn && dbaDropdown) {
              let closeTimeout: ReturnType<typeof setTimeout> | null = null;
              // Open dropdown on hover/focus
              const openDropdown = () => {
                if (closeTimeout) { clearTimeout(closeTimeout); closeTimeout = null; }
                dbaDropdown.classList.add('open');
                dbaOpen = true;
                if (dbaArrow) {
                  const arrowEl = dbaArrow as SVGElement;
                  arrowEl.style.transform = 'rotate(180deg)';
                }
              };
              const closeDropdown = () => {
                dbaDropdown.classList.remove('open');
                dbaOpen = false;
                if (dbaArrow) {
                  const arrowEl = dbaArrow as SVGElement;
                  arrowEl.style.transform = 'rotate(0deg)';
                }
              };
              dbaBtn.addEventListener('mouseenter', openDropdown);
              dbaBtn.addEventListener('focus', openDropdown);
              dbaBtn.addEventListener('mouseleave', (e) => {
                closeTimeout = setTimeout(() => {
                  if (!dbaDropdown.matches(':hover')) closeDropdown();
                }, 200);
              });
              dbaBtn.addEventListener('blur', () => {
                closeTimeout = setTimeout(closeDropdown, 200);
              });
              dbaDropdown.addEventListener('mouseenter', () => {
                if (closeTimeout) { clearTimeout(closeTimeout); closeTimeout = null; }
                openDropdown();
              });
              dbaDropdown.addEventListener('mouseleave', () => {
                closeTimeout = setTimeout(closeDropdown, 200);
              });
              dbaDropdown.addEventListener('blur', () => {
                closeTimeout = setTimeout(closeDropdown, 200);
              });

              // Connect Global DBA Widget to existing dropdown functionality
              const globalDbaWidget = document.querySelector('.global-widgets .dba-widget');
              if (globalDbaWidget) {
                  globalDbaWidget.addEventListener('click', function(e) {
                      e.preventDefault();
                      e.stopPropagation();
                      if (dbaOpen) {
                          closeDropdown();
                      } else {
                          openDropdown();
                      }
                  });

                  // Connect hover behavior
                  globalDbaWidget.addEventListener('mouseenter', openDropdown);
                  globalDbaWidget.addEventListener('mouseleave', (e) => {
                      closeTimeout = setTimeout(() => {
                          if (!dbaDropdown.matches(':hover')) closeDropdown();
                      }, 200);
                  });
              }
            }

            // Mobile menu functionality
            const mobileMenuToggle = document.querySelector('.mobile-menu-toggle');
            const mobileNav = document.querySelector('.mobile-nav');
            
            if (mobileMenuToggle && mobileNav) {
                mobileMenuToggle.addEventListener('click', () => {
                    mobileMenuToggle.classList.toggle('active');
                    mobileNav.classList.toggle('active');
                });
                
                // Close mobile menu when clicking on links
                const mobileNavLinks = document.querySelectorAll('.mobile-nav-link');
                mobileNavLinks.forEach(link => {
                    link.addEventListener('click', function() {
                        if (mobileMenuToggle && mobileNav) {
                            mobileMenuToggle.classList.remove('active');
                            mobileNav.classList.remove('active');
                        }
                    });
                });
            }
            
            // Smooth scrolling for navigation links
            const navLinks = document.querySelectorAll('.nav-link, .mobile-nav-link');
            navLinks.forEach(link => {
                link.addEventListener('click', (e) => {
                    const href = link.getAttribute('href');
                    if (href && href.startsWith('#')) {
                        e.preventDefault();
                        const targetId = href.substring(1);
                        const targetElement = document.getElementById(targetId);
                        if (targetElement) {
                            const offsetTop = targetElement.offsetTop - 80; // Account for sticky nav
                            window.scrollTo({
                                top: offsetTop,
                                behavior: 'smooth'
                            });
                        }
                    }
                });
            });
            
            // Add scroll effect to navigation
            const nav = document.querySelector('.trimesh-nav');
            
            if (nav instanceof HTMLElement) {
                window.addEventListener('scroll', function() {
                    const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                    
                    if (scrollTop > 100) {
                        nav.style.background = 'linear-gradient(135deg, rgba(255,255,255,0.98) 0%, rgba(248,249,250,0.98) 100%)';
                        nav.style.boxShadow = '0 6px 30px rgba(0,0,0,0.12)';
                    } else {
                        nav.style.background = 'linear-gradient(135deg, rgba(255,255,255,0.95) 0%, rgba(248,249,250,0.95) 100%)';
                        nav.style.boxShadow = '0 4px 20px rgba(0,0,0,0.08)';
                    }
                });
            }

            // Service drawer hover/click logic
            const serviceGroups = document.querySelectorAll('.drawer-service-group');
            serviceGroups.forEach(group => {
              group.addEventListener('mouseenter', () => {
                group.classList.add('expanded');
              });
              group.addEventListener('mouseleave', () => {
                group.classList.remove('expanded');
              });
              // For mobile/touch: toggle on click/focus
              group.addEventListener('click', (e) => {
                // Only expand if not already expanded
                if (!group.classList.contains('expanded')) {
                  serviceGroups.forEach(g => g.classList.remove('expanded'));
                  group.classList.add('expanded');
                  e.stopPropagation();
                }
              });
              group.addEventListener('blur', () => {
                group.classList.remove('expanded');
              });
            });
            document.addEventListener('click', function(e) {
              serviceGroups.forEach(g => g.classList.remove('expanded'));
            });

            // Global Services Widget Functionality
            const globalServicesWidget = document.querySelector('.global-widgets .services-widget');
            if (globalServicesWidget && drawer) {
                globalServicesWidget.addEventListener('click', function(e) {
                    e.preventDefault();
                    e.stopPropagation();
                    drawer.classList.add('open');
                    document.body.appendChild(drawerBackdrop);
                });
            }

            // Global Widgets Fade Behavior
            const globalWidgets = document.querySelector('.global-widgets');
            const trimeshNav = document.querySelector('.trimesh-nav') as HTMLElement;
            
            if (globalWidgets && trimeshNav) {
                window.addEventListener('scroll', function() {
                    const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                    const navOffset = trimeshNav.offsetTop - 100; // Start fading 100px before nav
                    
                    if (scrollTop >= navOffset) {
                        globalWidgets.classList.add('fading');
                    } else {
                        globalWidgets.classList.remove('fading');
                    }
                });
            }

            // Services Drawer - Company Tabs Functionality
            const companyTabs = document.querySelectorAll('.company-tab');
            const companyContents = document.querySelectorAll('.company-content');
            
            companyTabs.forEach(tab => {
                tab.addEventListener('click', () => {
                    const targetCompany = tab.getAttribute('data-company');
                    
                    // Remove active class from all tabs and contents
                    companyTabs.forEach(t => t.classList.remove('active'));
                    companyContents.forEach(c => c.classList.remove('active'));
                    
                    // Add active class to clicked tab and corresponding content
                    tab.classList.add('active');
                    const targetContent = document.querySelector(`.company-content[data-company="${targetCompany}"]`);
                    if (targetContent) {
                        targetContent.classList.add('active');
                    }
                });
            });

            // Services Drawer - Expandable Service Cards
            const serviceCards = document.querySelectorAll('.service-card');
            
            serviceCards.forEach(card => {
                const header = card.querySelector('.service-card-header');
                if (header) {
                    header.addEventListener('click', function() {
                        // Toggle expanded state
                        card.classList.toggle('expanded');
                        
                        // Optional: Close other expanded cards (uncomment if you want accordion behavior)
                        // serviceCards.forEach(otherCard => {
                        //     if (otherCard !== card) {
                        //         otherCard.classList.remove('expanded');
                        //     }
                        // });
                    });
                }
            });

            // Portfolio Filter Functionality
            const filterButtons = document.querySelectorAll('.filter-btn');
            const portfolioItems = document.querySelectorAll<HTMLElement>('.portfolio-item');
            
            filterButtons.forEach(button => {
                button.addEventListener('click', function(this: HTMLElement) {
                    const filter = this.getAttribute('data-filter');
                    
                    // Update active button
                    filterButtons.forEach(btn => btn.classList.remove('active'));
                    this.classList.add('active');
                    
                    // Filter portfolio items
                    portfolioItems.forEach(item => {
                        const category = item.getAttribute('data-category');
                        
                        if (filter === 'all' || category === filter) {
                            item.classList.remove('hidden');
                            setTimeout(() => {
                                item.style.opacity = '1';
                                item.style.transform = 'scale(1)';
                            }, 10);
                        } else {
                            item.style.opacity = '0';
                            item.style.transform = 'scale(0.9)';
                            setTimeout(() => {
                                item.classList.add('hidden');
                            }, 300);
                        }
                    });
                });
            });

            // Split Hero CTA Button Clicks
            const splitHeroCtas = document.querySelectorAll('.split-hero-cta');
            splitHeroCtas.forEach(cta => {
                cta.addEventListener('click', function(this: HTMLElement) {
                    const service = this.getAttribute('data-service');
                    if (service === 'operations') {
                        document.getElementById('drone-operations')?.scrollIntoView({ behavior: 'smooth' });
                    } else if (service === 'media') {
                        document.getElementById('media-marketing')?.scrollIntoView({ behavior: 'smooth' });
                    }
                });
            });
        });
    </script>
